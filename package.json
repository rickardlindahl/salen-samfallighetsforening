  "private": true,
  "scripts": {
    "db:connect": "pscale connect salen main --port 3309",
    "db:connect:shadow": "pscale connect salen shadowdb --port 3308",
    "build:tailwind": "tailwindcss --config tailwind.config.cjs --input src/app/global.css --output src/app/styles.css",
    "build:tailwind:watch": "npm run build:tailwind -- --watch",
